    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js"></script>
    <script language="javascript" type="text/javascript">
        $(document).ready(function () {
            var $acctname = $('#acctname');
            var options_acctname = {
                url: "/Global/GetAppVersion",
                type: "get",
                async: false
            };

            var $server = $('#server');
            var options_server = {
                url: "/Global/GetDatabaseServer",
                type: "get",
                async: false
            };

            var $database = $('#database');
            var options_database = {
                url: "/Global/GetDatabaseName",
                type: "get",
                async: false
            };

            $.ajax(options_acctname).done(function (data) {
                $acctname.text(data);
            });

            $.ajax(options_server).done(function (data) {
                $server.text(data);
            });

            $.ajax(options_database).done(function (data) {
                $database.text(data);
            });
        });
    </script>
