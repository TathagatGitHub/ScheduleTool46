<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        $('#ExcelLink').removeAttr('href');
        $('#ExcelLink').attr('href', "/ScheduleProposal/ExportToExcel?proposalId=@Model.ProposalInfo.ScheduleId&disc=" + getParameterByName('disc') + "&networkName=" + encodeURIComponent(getParameterByName('NetworkName')) + "&de=" + encodeURIComponent(getParameterByName('de')) + "&bt=" + encodeURIComponent(getParameterByName('bt')));
        var reporttable = $('#ProposalReport').DataTable({
            lengthMenu: [[6000], [6000]],
            ajax: {
                url: '/ScheduleProposal/GetProposalData',
                type: 'POST',
                data: function (d) {
                    d.proposalId = @Model.ProposalInfo.ScheduleId,
                    d.discountRate = parseFloat(getParameterByName('disc')),
                        d.pageLength = 200,
                        d.filterBuyTypes = getParameterByName('bt'),
                    d.filterDemoNames = getParameterByName('de'),
                    d.filterNetworkName = getParameterByName('NetworkName'),
                    d.ViewOnly = true
                }
            },
            paging: false,
            searching: false,
            info: false,
            columns: [
                {
                    targets: 0,
                    data: "propertyName",
                    class: "d-none d-sm-table-cell text-center",
                    render: $.fn.dataTable.render.ellipsis(33, true)
                },
                {
                    targets: 1,
                    data: "monday",
                    class: "d-none d-sm-table-cell text-center",
                    render: function (data, type, row, meta) {
                        if (data === true) {
                            return "<i class='fa fa-circle'></i>";
                        }
                        else {
                            return "";
                        }
                    }
                },
                {
                    targets: 2,
                    data: "tuesday",
                    class: "d-none d-sm-table-cell text-center",
                    render: function (data, type, row, meta) {
                        if (data === true) {
                            return "<i class='fa fa-circle'></i>";
                        }
                        else {
                            return "";
                        }
                    }
                },
                {
                    targets: 3,
                    data: "wednesday",
                    class: "d-none d-sm-table-cell text-center",
                    render: function (data, type, row, meta) {
                        if (data === true) {
                            return "<i class='fa fa-circle'></i>";
                        }
                        else {
                            return "";
                        }
                    }
                },
                {
                    targets: 4,
                    data: "thursday",
                    class: "d-none d-sm-table-cell text-center",
                    render: function (data, type, row, meta) {
                        if (data === true) {
                            return "<i class='fa fa-circle'></i>";
                        }
                        else {
                            return "";
                        }
                    }
                },
                {
                    targets: 5,
                    data: "friday",
                    class: "d-none d-sm-table-cell text-center",
                    render: function (data, type, row, meta) {
                        if (data === true) {
                            return "<i class='fa fa-circle'></i>";
                        }
                        else {
                            return "";
                        }
                    }
                },
                {
                    targets: 6,
                    data: "saturday",
                    class: "d-none d-sm-table-cell text-center",
                    render: function (data, type, row, meta) {
                        if (data === true) {
                            return "<i class='fa fa-circle'></i>";
                        }
                        else {
                            return "";
                        }
                    }
                },
                {
                    targets: 7,
                    data: "sunday",
                    class: "d-none d-sm-table-cell text-center",
                    render: function (data, type, row, meta) {
                        if (data === true) {
                            return "<i class='fa fa-circle'></i>";
                        }
                        else {
                            return "";
                        }
                    }
                },
                {
                    targets: 8,
                    data: "startToEndTime",
                    class: "d-none d-sm-table-cell text-center"
                },
                {
                    targets: 9,
                    data: "buyTypeCode",
                    class: "d-none d-sm-table-cell text-center"
                },
                {
                    targets: 10,
                    data: "dayPartCd",
                    class: "d-none d-sm-table-cell text-center"
                },
                {
                    targets: 11,
                    data: "spotLen",
                    class: "d-none d-sm-table-cell text-center",
                    render: function (data, type, row, meta) {
                        if (data === 120) {
                            return "<span class='badge badge-pill badge-danger'>" + data + "</span>";
                        }
                        else if (data === 60) {
                            return "<span class='badge badge-pill badge-primary'>" + data + "</span>";
                        }
                        else if (data === 15) {
                            return "<span class='badge badge-pill badge-warning'>" + data + "</span>";
                        }
                        else {
                            return "<span class='badge badge-pill badge-success'>" + data + "</span>";
                        }
                    }
                },
                {
                    targets: 12,
                    data: "demoName",
                    class: "d-none d-sm-table-cell text-center",
                    render: $.fn.dataTable.render.ellipsis(33, true)
                },
                {
                    targets: 13,
                    data: "rateAmt",
                    class: "d-none d-sm-table-cell text-right",
                    render: $.fn.dataTable.render.number(',', '.', 2, '$')
                },
                {
                    targets: 14,
                    data: "impressions",
                    class: "d-none d-sm-table-cell text-center",
                    visible: '@Model.IsDRBT.ToString()' == 'True' ? false : true,
                    render: $.fn.dataTable.render.number(',', '.', 2, '')
                },
                {
                    targets: 15, /* CPM */
                    data: "cpm",
                    class: "d-none d-sm-table-cell text-right",
                    visible: '@Model.IsDRBT.ToString()' == 'True' ? false : true,
                    render: $.fn.dataTable.render.number(',', '.', 2, '$')

                },
                {
                    targets: 16,
                    data: "totalSpots",
                    class: "d-none d-sm-table-cell text-center",
                    createdCell: function (td, cellData, rowData, row, col) {
                        $(td).attr('id', 'totalSpots_' + rowData.scheduleLineId);
                    }
                },
                {
                    targets: 17,
                    data: "wk01_Spots",
                    class: "d-none d-sm-table-cell text-center table-secondary",
                    visible: '@Model.Quarter.Wk01_Date.ToString()' == '' ? false : true,
                    render: function (data, type, row, meta) {
                        if (data == 0)
                            return '';
                        else
                            return data;
                    }
                },
                {
                    targets: 18,
                    data: "wk02_Spots",
                    class: "d-none d-sm-table-cell text-center table-secondary",
                    visible: '@Model.Quarter.Wk02_Date.ToString()' == '' ? false : true,
                    render: function (data, type, row, meta) {
                        if (data == 0)
                            return '';
                        else
                            return data;
                    }
                },
                {
                    targets: 19,
                    data: "wk03_Spots",
                    class: "d-none d-sm-table-cell text-center table-secondary",
                    visible: '@Model.Quarter.Wk03_Date.ToString()' == '' ? false : true,
                    render: function (data, type, row, meta) {
                        if (data == 0)
                            return '';
                        else
                            return data;
                    }
                },
                {
                    targets: 20,
                    data: "wk04_Spots",
                    class: "d-none d-sm-table-cell text-center table-secondary",
                    visible: '@Model.Quarter.Wk04_Date.ToString()' == '' ? false : true,
                    render: function (data, type, row, meta) {
                        if (data == 0)
                            return '';
                        else
                            return data;
                    }
                },
                {
                    targets: 21,
                    data: "wk05_Spots",
