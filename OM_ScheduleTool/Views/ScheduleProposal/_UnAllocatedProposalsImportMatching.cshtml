<style type="text/css">

    div.scroll {
        width: 200px;
        height: 150px;
        overflow-x: hidden;
        overflow-y: auto;
        text-align: left;
        padding: 20px;
    }

    /*Overlay Code Style*/
    .overlay {
        height: 100%;
        width: 0;
        position: fixed;
        z-index: 1000;
        top: 0;
        left: 0;
        background-color: rgb(0,0,0);
        background-color: rgba(0,0,0, 0.9);
        overflow-x: hidden;
        transition: 0.5s;
        opacity: 0.5;
    }

    .overlay-content {
        position: relative;
        top: 25%;
        width: 100%;
        text-align: center;
        margin-top: 30px;
    }

    .overlay a {
        padding: 8px;
        text-decoration: none;
        font-size: 36px;
        color: #818181;
        display: block;
        transition: 0.3s;
    }

        .overlay a:hover, .overlay a:focus {
            color: #f1f1f1;
        }

    .overlay .closebtn {
        position: absolute;
        top: 20px;
        right: 45px;
        font-size: 60px;
    }
     #tblTotal th,
     #tblTotal td {width:100px;padding:3px; font-size:14px;}
     .active-tab{ box-shadow: inset 0 -2px #42a5f5; }

    table.display {
        margin: 0 auto;
        width: 100%;
        clear: both;
        border-collapse: collapse;
        table-layout: fixed;
        word-wrap: break-word;
    }
    .matchedcell
    {
        background-color: #9ccc65;
    }
    .unmatchedcell
    {
        background-color:yellow;
    }
    .emptycell{
    background-color:darkgray;
    }
    .valuecell
    {
        background-color:lightblue;
    }

    .hideThis{
    display:none;
    }
    /*ST-720*/
    .fully-actualized-header {
        background-color: #F79646 !important;
    }

    .fully-actualized {
        background-color: #d5dde5 !important;
    }

    .fully-actualized-text {
        color: #899499 !important;
    }
</style>

<div class="modal fade" id="modal-UnAllocatedProposalsMatching" tabindex="-1" role="dialog" aria-labelledby="modal-UnAllocatedProposalsMatching" aria-hidden="true">
