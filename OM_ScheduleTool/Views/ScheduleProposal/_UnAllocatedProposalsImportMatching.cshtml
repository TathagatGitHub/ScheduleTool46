        <div class="modal-content">
            <div class="block block-themed block-transparent mb-0" style="height:100%;">
                <div class="block-header bg-primary-dark">
                    <h3 class="block-title" id="MatchingHeadingData"></h3>
                    <div class="block-options">
                        <button type="button" id="btnDRCloseTop"  class="btn-block-option" data-dismiss="modal" aria-label="Close">
                            <i class="si si-close"></i>
                        </button>
                    </div>
                </div>                
                <div id="DivOverLayPopoupMatching" class="overlay" style="display: none; width: 100%!important;z-index:10000000;">
                    <div style="text-align:center;position:absolute;vertical-align:middle;color:blue;height:150px;width:150px;left:50%;top:50%;margin-left:-75px;margin-top:-75px;">
                        <img src="~/img/loadingImg.gif" style="height:60px;width:60px;" />
                    </div>
                </div>
                <div class="block-content block-content-full" id="divUnAllocatedNetworksMatching"
                     name="divUnAllocatedNetworksMatching" style="overflow: scroll;">
                    <div style="justify-content: center;" class="row">
                        <table class="table-bordered table-vcenter js-table-checkable table-hover compact nowrap display"
                               style="border: 1px solid #e5e5e5;width:100%;" id="MultiMatchedProposalsMatching" name="MultiMatchedProposalsMatching">
                            <thead id="thMultiMatchedProposalsMatching">
                            </thead>
                            <tbody id="tbMultiMatchedProposalsMatching">
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
                <div class="modal-footer">
                <button type="button" id="btnDRCloseAll" class="btn btn-alt-secondary" data-dismiss="modal">Close</button>
                <input type="button" id="btnbackImportLoad" value="Back" class="btn btn-alt-secondary" onclick="BackToLoadImportscreen()" data-type="view" />
                <input type="button" id="btnConfirmMatches" value="Confirm Matches" class="btn btn-alt-success" onclick="ConfirmMathes()" data-type="view" disabled/>                
               </div>
        </div>
    </div>
</div>


