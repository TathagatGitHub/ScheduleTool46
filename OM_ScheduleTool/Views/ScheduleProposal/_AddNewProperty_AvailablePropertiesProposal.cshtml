@model ProposalViewModel
<table class="table display" cellspacing="0" cellpadding="0" id="AddNewPropertyTable" name="AddNewPropertyTable" style="width:2250px;max-width:2250px;">
    <thead style="background-color:#42A4F4">
        <tr>
            <th class="d-none d-sm-table-cell text-left" style="border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;"><input type="checkbox" name="select_all" value="1" id="properties-select-all" style="width:16px;height:15px;"></th>
            <th class="d-none d-sm-table-cell text-left" style="color: white;width:300px!important;text-transform: capitalize;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">Property Name</th>
            <th class="d-none d-sm-table-cell text-left" style="color: white;width:100px!important;text-transform: capitalize;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">Start-End Time</th>
            <th class="d-none d-sm-table-cell text-left" style="color: white;width:25px!important;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">DP</th>
            <th class="d-none d-sm-table-cell text-left" style="color: white;width:35px!important;text-transform: capitalize;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">Days</th>
            <th class="d-none d-sm-table-cell text-center" style="color: white;width:55px!important;text-transform: capitalize;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">Len</th>
            <th class="d-none d-sm-table-cell text-right" style="color: white;width:80px!important;text-transform: capitalize;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">Rate</th>
            <th class="d-none d-sm-table-cell text-right" style="color: white;width:35px!important;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">Imp</th>
            <th class="d-none d-sm-table-cell text-right" style="color: white;width:35px!important;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">CPM</th>
            <th class="d-none d-sm-table-cell text-left" style="color: white;width:80px!important;text-transform: capitalize;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">Status</th>
            <th class="d-none d-sm-table-cell text-right" style="color: white;width:85px!important;text-transform: capitalize;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">Eff-Exp</th>
            <th class="d-none d-sm-table-cell text-right" style="color: white;width:80px!important;text-transform: capitalize;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">Revised Date</th>
            <th class="d-none d-sm-table-cell text-left" style="color:white;width:80px!important;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">StartTime</th>
            <th class="d-none d-sm-table-cell text-left" style="color:white;width:80px!important;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">EndTime</th>
            <th class="d-none d-sm-table-cell text-left" style="color: white;width:30px!important;text-transform: capitalize;border-right: 1px solid #fff;border-bottom: 1px solid #42A4F4;font-size: 17px">SP Buy</th>
            <th class="d-none d-sm-table-cell text-left" style="color: white;width:25px!important;border-bottom: 1px solid #42A4F4;font-size: 17px">BT</th>
        </tr>
    </thead>
</table>


