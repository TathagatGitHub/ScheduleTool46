<style type="text/css">
    .dropdown-menu {
        margin-left: -80px !important;
    }

        .dropdown-menu ul li {
            cursor: pointer;
        }

            .dropdown-menu ul li a {
                cursor: pointer;
            }

    .content {
        width: 90% !important;
    }

    .table thead tr th {
        text-transform: none;
    }

    #tblPlans_filter {
        display: none;
    }

    .overbudget {
        color: red !important;
    }

    /*Overlay Code Style*/
    .overlay {
        height: 100%;
        width: 0;
        position: fixed;
        z-index: 1000;
        top: 0;
        left: 0;
        background-color: rgb(0,0,0);
        background-color: rgba(0,0,0, 0.9);
        overflow-x: hidden;
        transition: 0.5s;
        opacity: 0.5;
    }

    .overlay-content {
        position: relative;
        top: 25%;
        width: 100%;
        text-align: center;
        margin-top: 30px;
    }

    .overlay a {
        padding: 8px;
        text-decoration: none;
        font-size: 36px;
        color: #818181;
        display: block;
        transition: 0.3s;
    }

        .overlay a:hover, .overlay a:focus {
            color: #f1f1f1;
        }

    .overlay .closebtn {
        position: absolute;
        top: 20px;
        right: 45px;
        font-size: 60px;
    }

    .container-fluid#tblPlans4Q_wrapper, .container-fluid#tblPlans1Q_wrapper, .container-fluid#tblPlans2Q_wrapper, .container-fluid#tblPlans3Q_wrapper {
        padding-right: 0;
        padding-left: 0;
    }

    .content {
        width: 100% !important;
    }

    .content-header {
        margin-left: 0 !important;
        padding: 0 !important;
    }

    #page-footer {
        width: 100% !important;
        max-width: 2000px;
    }



        #page-footer > div:first-child {
            float: left;
            width: 100% !important
        }

    #page-container.main-content-boxed > #main-container .content, #page-container.main-content-boxed > #page-footer .content, #page-container.main-content-boxed > #page-header .content, #page-container.main-content-boxed > #page-header .content-header {
        max-width: 2000px;
    }

    .row-published {
        /*background-color:#d3eafd !important;*/
        /*background-color:#E0FFFF!important;*/
        background-color: #ebf5df !important;
    }

    .mpCopy-Dropdown {
        position: absolute;
        display: inline-block;        
    }

    .mpCopy-DropdownContent {
        display: none;
        position: absolute;
        background-color: #f1f1f1;
        min-width: 94px;
        z-index: 1;
        margin-top: 32px;
        margin-left: -150px;
        border-radius: 3px;
        text-align: left;
        color: #5c852c;
        background-color: #ebf5df;
        border-color: #ebf5df;
        height: 34px;
        font-family: -apple-system,system-ui,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif;
        font-weight: 500;
    }

    .mpCopy-DropdownContent a {
        color: #171717;
        padding: 8px 14px;
        text-decoration: none;
        display: block;
    }

    .mpCopy-DropdownContent a:hover {
        background-color: #cde0b6;
        min-width: 94px;
        border-radius: 3px;
        text-align: left;
        border-color: #ebf5df;
        height: 34px;
        font-family: -apple-system,system-ui,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif;
        font-weight: 500;
